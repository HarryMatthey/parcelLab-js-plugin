{
  "name": "parcelLab-js-plugin",
  "version": "1.0.0",
  "description": "JavaScript plugin used to retrieve and display trackings from parcelLab on any given webpage.",
  "main": "src/js/index.js",
  "browser": "build/parcelLab.min.js",
  "scripts": {
    "start": "npm-run-all -p dev:*",
    "test": "make test",
    "build": "make bundle",
    "deploy": "make deploy",
    "dev:serve-test": "serve test/ -p 4000",
    "dev:serve-bundle": "serve build/ -p 4001",
    "dev:watch-build": "webpack -d --progress --colors --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/parcelLab/parcelLab-js-plugin.git"
  },
  "keywords": [
    "parcellab",
    "trackings"
  ],
  "author": "Julian Krenge, Oscar Reyes, Andrej Fritz",
  "license": "MIT",
  "homepage": "https://www.parcellab.com",
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "npm-run-all": "^3.0.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.2",
    "serve": "^1.4.0",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "bel": "^5.1.5",
    "cash-dom": "^1.3.4",
    "country-language": "^0.1.7",
    "countryjs": "^1.5.2",
    "nanobus": "^4.3.1",
    "nanomorph": "^5.1.3",
    "nanostore": "^1.0.0",
    "raven-js": "^3.5.1"
  }
}
