{
  "name": "parcelLab-js-plugin",
  "version": "1.0.0",
  "description": "JavaScript plugin used to retrieve and display trackings from parcelLab on any given webpage.",
  "main": "src/js/index.js",
  "browser": "build/parcelLab.min.js",
  "scripts": {
    "start": "parcel test/index.html -p 4000",
    "build": "parcel build src/index.js",
    "deploy": "npm run build && make deploy && make deploy-v3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/parcelLab/parcelLab-js-plugin.git"
  },
  "keywords": [
    "parcellab",
    "trackings"
  ],
  "author": "Julian Krenge, Oscar Reyes, Andrej Fritz",
  "license": "MIT",
  "homepage": "https://www.parcellab.com",
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "node-sass": "^4.8.3",
    "parcel-bundler": "^1.7.1"
  },
  "dependencies": {
    "bel": "^5.1.5",
    "cash-dom": "^1.3.4",
    "nanomorph": "^5.1.3",
    "nanostore": "^1.2.0",
    "raven-js": "^3.5.1"
  }
}
