{{#equals type "maps"}}
  <!-- Maps  -->
  <a href="{{generateMapsLink address}}" title="{{address}}" target="_blank">
    <div class="pl-box-body pl-box-maps" style="{{{generateMapBG address}}}">
      <div class="pl-box-action-info">
        <i class="fa fa-road fa-2x" style="margin-bottom: 10px;"></i>
        {{address}}
      </div>
      <div class="pl-address-info">
        <div>{{address}}</div>
      </div>
    </div>
  </a>
{{else}}
  {{#if type}}
    <div class="pl-box-body" style="padding: 15px;">
      {{#if label}}<h3 style="text-align:center;">{{label}}</h3>{{/if}}

      <!-- Vote -->
      {{#equals type "vote-courier"}}
        <div class="rating-body">
            <div class="pl-courier-vote up" data-vote="up">
              <i class="fa fa-thumbs-o-up"></i>
            </div>
            <div class="pl-courier-vote down" data-vote="down">
              <i class="fa fa-thumbs-o-down"></i>
            </div>
        </div>
      {{/equals}}

      {{#equals type "prediction"}}
        prediction go here
      {{/equals}}

    </div>
  {{/if}}
{{/equals}}
